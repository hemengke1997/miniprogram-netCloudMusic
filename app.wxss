/**app.wxss**/
@import './assets/font/iconfont.wxss';
@import './assets/css/common.wxss';

page {
  background-size: 100% 100%;
  height: 100%;
  font-size: 32rpx;
  color: #333;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
} 

iamge {
  width: 0;
  height: 0;
}

button {
  background-color: transparent;
  border-radius: 0rpx;
  padding: 0;
  margin: 0;
  display: inline-block;
  line-height: 1;
}

button::after {
  display: none;
  border: none;
}
